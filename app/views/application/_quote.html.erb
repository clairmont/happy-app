<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Quote</th>
      <th>Author</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.quote %></td>
        <td><%= quote.author %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
