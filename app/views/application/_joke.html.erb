<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Joke</th>
      <th>Punchline</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <% @jokes.each do |joke| %>
      <tr>
        <td><%= joke.joke %></td>
        <td><%= joke.punchline %></td>
      </tr>
   <% end %>
  </tbody>
</table>
<br>
